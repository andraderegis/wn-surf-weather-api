    "api"
  ],
  "author": "Reginaldo Andrade",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andraderegis/wn-surf-weather-api/issues"
  },
  "homepage": "https://github.com/andraderegis/wn-surf-weather-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.11.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  }
